{
	"name": "hda-typo3/pressrelations-news-import",
	"type": "typo3-cms-extension",
	"description": "Import of news from PressRelations",
	"authors": [
		{
			"name": "Georg Ringer",
			"role": "Developer"
		}
	],
	"license": "GPL-2.0-or-later",
	"require": {
		"php": "^8.1",
		"georgringer/news": "^10 || ^11",
		"typo3/cms-core": "^11 || ^12"
	},
	"suggest": {
		"typo3/cms-scheduler": "Import"
	},
	"autoload": {
		"psr-4": {
			"GeorgRinger\\PressrelationsNewsImport\\": "Classes"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "pressrelations_news_import"
		}
	}
}
